{
  "name": "ui-eye-root",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "web",
    "server"
  ],
  "scripts": {
    "dev": "npm run dev -w web",
    "build": "npm run build -w web",
    "server": "npm run server -w server",
    "test": "npm run test -w web",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "install:all": "npm install",
    "doctor": "node server/scripts/env-check.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}